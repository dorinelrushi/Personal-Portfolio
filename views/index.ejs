    <!--ending cdn-->
  <script>
 
  var clicked = false;
   var bars = document.querySelector(".bars");
   var bars_up = document.querySelector(".bars-up");
   var bars_down = document.querySelector(".bars-down");
   var mobile_nav = document.querySelector('.mobile-nav');

   bars.addEventListener("click",()=>{
      if(!clicked){
        clicked = true;
        mobile_nav.style.display = 'block';
        gsap.to(bars_down,{rotate : -46,duration : 0.25})
        gsap.to(bars_up,{rotate : 46, y : 5.5,duration : 0.25})

      }else{
        clicked = false;
        mobile_nav.style.display = 'none';
        gsap.to(bars_down,{rotate : 0,duration : 0.25})
        gsap.to(bars_up,{rotate : 0, y : 0,duration : 0.25})

      }
   })



  </script>
